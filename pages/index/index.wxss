/**index.wxss**/
.session-title{
  font-size: 14px;
  background: #f5f5f5;
  padding: 10px;
  color: #999;
}
.flex-wrp{
  width:100%;
  flex-direction:row;
  display: flex;
  flex-wrap: wrap; 
  padding-top: 1px;
  background: #f9f9f9;
}
.flex-item{
  width:25%;
  height:100px;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 12px;
  color: #666;
  margin-top:-1px;
  margin-left:-1px;
  padding: 20px 0;
  box-sizing:border-box;
}

.flex-item image,.flex-item text{
  flex:1;
  display: flex;
  justify-content: center;
  align-items: center;
}
